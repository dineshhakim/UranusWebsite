@model IEnumerable<Uranus.Domain.Entities.Company>

@{
    ViewBag.Title = "Index";
}



<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                Company
                <small>Index</small>
            </h1>
            <ol class="breadcrumb">
                <li>
                    @Html.ActionLink("Home", "Index", "Home")
                </li>
                <li class="active">Company</li>
            </ol>
        </div>
    </div>
    <div class="row">
        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.CompanyName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CompanyType)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CompanyAddress)
                </th>

                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.CompanyName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CompanyType)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CompanyAddress)
                    </td>

                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>